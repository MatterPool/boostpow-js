{
  "name": "boostpow",
  "version": "0.0.0-development",
  "description": "BoostPow Javascript SDK",
  "main": "dist/lib/index.js",
  "types": "dist//lib/index.d.ts",
  "typings": "dist/lib/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build",
    "test": "mocha --reporter spec test/*.js test*.ts",
    "test_submitBoostJob": "mocha --reporter spec --grep 'submitBoostJob'",
    "test_BoostPowMetadata": "mocha --reporter spec --grep 'BoostPowMetadata'",
    "test_BoostPowJobProof": "mocha --reporter spec --grep 'BoostPowJobProof'",
    "test_search": "mocha --reporter spec --grep 'Graph Search'",
    "test_signal": "mocha --reporter spec --grep 'Boost.?Signal'",
    "test_curr": "mocha --reporter spec --grep 'test_curr'",
    "test_decode": "mocha --reporter spec --grep 'test_decode'",
    "test_file": "mocha --reporter spec --grep 'filepay'",
    "prepare": "npm run build",
    "commit": "npx cz",
    "semantic-release": "npx semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ProofOfWorkCompany/boostpow-js.git"
  },
  "author": "",
  "keywords": [
    "bitcoin",
    "bsv",
    "bitcoin-sv",
    "metanet",
    "boost",
    "proof-of-work",
    "pow",
    "boost-pow",
    "boost-protocal",
    "hashcash",
    "blockchain",
    "satoshi",
    "satoshi-vision"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@types/mocha": "^9.1.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "browserify-shim": "^3.8.14",
    "bsv": "^1.5.6",
    "chai": "^4.2.0",
    "cz": "^1.8.2",
    "cz-conventional-changelog": "^3.3.0",
    "gulp": "^4.0.2",
    "gulp-batch": "^1.0.5",
    "gulp-clean-css": "^3.10.0",
    "gulp-clone": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-cssimport": "^6.0.1",
    "gulp-foreach": "^0.1.0",
    "gulp-header": "^2.0.7",
    "gulp-htmlmin": "^5.0.1",
    "gulp-less": "^4.0.1",
    "gulp-livereload": "^4.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-uglify-es": "^1.0.4",
    "gulp-watch": "^5.0.1",
    "mocha": "^8.4.0",
    "streamqueue": "^1.1.2",
    "ts-node": "^10.7.0",
    "tsify": "^4.0.1",
    "typescript": "^3.9.6",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "semantic-release": "^19.0.2"
  },
  "dependencies": {
    "@types/node": "^13.7.7",
    "axios": "^0.21.1",
    "crypto-random-string": "^3.1.0",
    "i": "^0.3.6",
    "mattercloudjs": "^1.0.12",
    "randombytes": "^2.1.0"
  },
  "files": [
    "dist/*"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
